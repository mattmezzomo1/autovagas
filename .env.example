# Configurações do servidor
PORT=3000
NODE_ENV=development

# Configurações do banco de dados
DB_HOST=localhost
DB_PORT=5432
DB_NAME=autovagas
DB_USER=postgres
DB_PASSWORD=postgres

# Configurações do JWT
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# Configurações do LinkedIn
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret
LINKEDIN_REDIRECT_URI=http://localhost:3000/api/auth/linkedin/callback

# Credenciais de teste para o scraper do LinkedIn
LINKEDIN_TEST_USERNAME=your_linkedin_username
LINKEDIN_TEST_PASSWORD=your_linkedin_password

# Configurações do serviço de resolução de captchas
CAPTCHA_SOLVER_API_KEY=your_captcha_solver_api_key

# Proxy Configuration
PROXY_ENABLED=true

# Bright Data Configuration
BRIGHTDATA_ENABLED=true
BRIGHTDATA_API_KEY=your_brightdata_api_key
BRIGHTDATA_ZONE_ID=your_brightdata_zone_id

# Oxylabs Configuration
OXYLABS_ENABLED=true
OXYLABS_USERNAME=your_oxylabs_username
OXYLABS_PASSWORD=your_oxylabs_password

# SmartProxy Configuration
SMARTPROXY_ENABLED=true
SMARTPROXY_USERNAME=your_smartproxy_username
SMARTPROXY_PASSWORD=your_smartproxy_password

# Local Proxy Configuration
LOCAL_PROXY_ENABLED=true
LOCAL_PROXY_LIST_PATH=./proxies.txt

# Scraper Configuration
MAX_CONCURRENT_TASKS=10
DEFAULT_SCRAPER_TIMEOUT=60000
USER_AGENT_ROTATION=true
MIN_SCRAPER_DELAY=1000
MAX_SCRAPER_DELAY=5000

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL=3600000
CACHE_MAX_SIZE=1000

# User Tiers Configuration
BASIC_MAX_SEARCHES_PER_DAY=10
BASIC_MAX_JOB_DETAILS_PER_DAY=50
BASIC_USE_SERVER_SIDE_SCRAPING=false

PLUS_MAX_SEARCHES_PER_DAY=30
PLUS_MAX_JOB_DETAILS_PER_DAY=150
PLUS_USE_SERVER_SIDE_SCRAPING=true

PREMIUM_MAX_SEARCHES_PER_DAY=100
PREMIUM_MAX_JOB_DETAILS_PER_DAY=500
PREMIUM_USE_SERVER_SIDE_SCRAPING=true

# Scaling Configuration
MAX_WORKERS=0
AUTO_SCALING=true
MIN_WORKERS=1
LOAD_BALANCING_STRATEGY=round-robin
STICKY_SESSIONS=false

# Redis Configuration
REDIS_ENABLED=true
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# WebSocket Configuration
WEBSOCKET_ENABLED=true
WS_PING_TIMEOUT=60000
WS_PING_INTERVAL=25000

# OpenAI Configuration for Interview Training
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Stripe Configuration
REACT_APP_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here
